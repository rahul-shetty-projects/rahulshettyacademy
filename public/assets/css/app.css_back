.pricing-container .pricing-footer button {
    border-radius: 50px;
    padding: 7px 20px;
    margin: 10px 0px;
    border: 2px solid #ec5252;
    background: none;
    color: #ec5252;
}

.pricing-container .pricing-footer button:hover {
    color: #ffffff;
    background: #ec5252;
}

#job-container .item-block{
    border: 2px solid #efefef;
    margin: 10px 0px;
}

#job-container .job-post-header {
    padding: 20px;
    border-bottom: 1px solid #efefef;
}

#job-container .job-post-header img {
    width: 100px;
    margin-right: 30px;
    float: left;
}

#job-container .job-post-header > div {
    display: inline-block;
    padding-top: 10px;
}

#job-container .job-post-header > div.pull-right .btn{
    background: none;
    color: #29303b;
    border-radius: 50px;
    border: 2px solid #29303b;
    padding: 10px 25px;
    text-transform: uppercase;
    font-weight: bold;
}

#job-container .job-post-header > div.pull-right .btn:hover {
    color: #fff;
    background: #ec5252;
    border-color: #ec5252;
}

#job-container .job-post-header > div > div {
    font-weight: normal;
    font-size: 14px;
}

#job-container .job-post-header > div > div > span{
    font-weight: 600;
    margin-right: 20px;
}

#job-container .job-post-header > div > a > h4{
    font-size: 24px;
    color: #29303b;
}

#job-container .job-post-header > div > h5{
    font-size: 20px;
}

#job-container .job-post-body {
    padding: 20px;
}

#job-container .job-post-body li{
    list-style: none;
    margin: 10px 0 10px 25px;
    position: relative;
    font-size: 16px;
    color: #666;
}

#job-container .job-post-body li:before{
    width: 8px;
    height: 8px;
    border-radius: 50%;
    position: absolute;
    left: -25px;
    top: 50%;
    -webkit-transform: translateY(-50%);
    -moz-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    -o-transform: translateY(-50%);
    transform: translateY(-50%);
    background: #ec5252;
    content: '';
    display: block;
}

#job-container .job-post-footer {
    margin: 0;
    padding: 5px;
    border-top: 1px solid #e3e3e3;
    color: #999;
    text-align: center;
}

#job-container .job-post-footer i {
    padding-right: 10px;
}

